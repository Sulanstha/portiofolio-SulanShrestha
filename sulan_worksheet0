        "def sum_nested_list(n):\n",
        "    total = 0\n",
        "    for x in n:\n",
        "        if isinstance(x, list):\n",
        "            total += sum_nested_list(x)\n",
        "        else:\n",
        "            total += x\n",
        "    return total\n",
        "\n",
        "sum_nested_list([1, [2, [3,4], 5], 6, [7,8]])"
      ]
    },
    {
      "cell_type": "code",
