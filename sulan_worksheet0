        "\n",
        "def calculate_directory_size(d):\n",
        "    total = 0\n",
        "    for k,v in d.items():\n",
        "        if isinstance(v, dict):\n",
        "            total += calculate_directory_size(v)\n",
        "        else:\n",
        "            total += v\n",
        "    return total\n",
