      "source": [
        "def generate_permutations(s):\n",
        "    if len(s) == 1:\n",
        "        return [s]\n",
        "    perms = []\n",
        "    for i in range(len(s)):\n",
        "        for p in generate_permutations(s[:i] + s[i+1:]):\n",
        "            perms.append(s[i] + p)\n",
        "    return list(dict.fromkeys(perms))\n",
        "\n",
        "generate_permutations(\"abc\")"
      ]
    },
