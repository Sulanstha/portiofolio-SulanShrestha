      "source": [
        "def min_coins(coins, amount):\n",
        "    dp = [float('inf')] * (amount+1)\n",
        "    dp[0] = 0\n",
        "    for c in coins:\n",
        "        for i in range(c, amount+1):\n",
        "            dp[i] = min(dp[i], dp[i-c] + 1)\n",
        "    return dp[amount] if dp[amount] != float('inf') else -1\n",
        "\n",
        "min_coins([1,2,5], 11)"
      ]
    },
